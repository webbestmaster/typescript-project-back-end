{
  "name": "typescript-project-back-end",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webbestmaster/typescript-project-back-end.git"
  },
  "bugs": {
    "url": "https://github.com/webbestmaster/typescript-project-back-end/issues"
  },
  "homepage": "https://github.com/webbestmaster/typescript-project-back-end#readme",
  "license": "MIT",
  "author": "",
  "version": "0.0.0",
  "description": "",
  "keywords": [
    "typescript-project",
    "empty"
  ],
  "main": "dist/index.js",
  "types": "dist/library.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-cli serve --progress",
    "start:transpile-only": "cross-env NODE_ENV=development TS_TRANSPILE_ONLY=true webpack-cli serve --progress",
    "tsc-watch": "tsc -p ./tsconfig.json --watch",
    "build": "cross-env NODE_ENV=production NODE_OPTIONS=--max-old-space-size=1536 webpack --progress",
    "build-library": "cross-env NODE_ENV=production IS_BUILD_LIBRARY=true NODE_OPTIONS=--max-old-space-size=1536 webpack --progress && npm run tsc-library && node webpack/util/library/css.js",
    "check": "npm run ts-coverage && npm run tsc && npm run test:unit && npm run eslint && npm run stylelint",
    "check:fix": "npm run prettier && npm run ts-coverage && npm run tsc && npm run test:unit && npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint ./**.scss",
    "stylelint:fix": "stylelint ./**.scss --fix",
    "prettier": "prettier --write \"./**/*.(js|ts|tsx)\" --loglevel warn",
    "tsc": "tsc -p ./tsconfig.json",
    "tsc-library": "tsc -p ./tsconfig.library.json",
    "ts-coverage": "typescript-coverage-report --threshold=100 --strict=true --project src/",
    "test:e2e": "jest --config='test-e2e/config/jest.config.js' ./test-e2e/ --runInBand --silent --passWithNoTests",
    "test:unit": "jest --config='test-unit/config/jest.config.js' src/ --coverage --silent --passWithNoTests",
    "pre-commit-hook": "npm run check",
    "_postinstall": "cp ./git-hooks/pre-commit ./.git/hooks"
  },
  "dependencies": {
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "ajv": "^8.6.3",
    "markdown-pro": "^1.0.21",
    "react": "^17.0.2",
    "react-audio-player-pro": "^1.1.16",
    "react-dom": "^17.0.2",
    "react-localization-library": "^0.0.17",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-router-dom-hook": "^0.0.12",
    "react-system-hook": "^0.0.13"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-strict-mode": "^7.14.5",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/puppeteer": "^5.4.4",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.0",
    "css-modules-typescript-loader": "^4.0.1",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "eslint-plugin-typescript-sort-keys": "^2.0.0",
    "eslint-plugin-unicorn": "^37.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.3.1",
    "jest-transform-stub": "^2.0.0",
    "mini-css-extract-plugin": "^2.4.3",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss": "^8.3.11",
    "postcss-scss": "^4.0.1",
    "prettier": "^2.4.1",
    "puppeteer": "^10.4.0",
    "raw-loader": "^4.0.2",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "style-loader": "^3.3.1",
    "stylelint": "^14.0.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-scss": "^4.0.0",
    "terser-webpack-plugin": "^5.2.4",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "typescript-coverage-report": "^0.6.1",
    "webpack": "^5.60.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0"
  }
}
